  <PropertyGroup Label="Globals">
    <Configuration Condition="$(Configuration) == ''">Release</Configuration>
    <Platform Condition="$(Platform) == ''">x64</Platform>
    <ProjectGuid>{{{guid!s}}}</ProjectGuid>
    <RootNamespace>{project.target_name}</RootNamespace>
    <TargetName>{project.target_name}</TargetName>
    <VersionString>{distinfo[version]}</VersionString>
    <DistInfoName>{distinfo[name]}-{distinfo[version]}.dist_info</DistInfoName>
    <SourceDirRoot>$(MSBuildThisFileDirectory)</SourceDirRoot>
    <SourceDir>$([msbuild]::EnsureTrailingSlash($(SourceDirRoot)))$([msbuild]::EnsureTrailingSlash(`{project.root}`))</SourceDir>
    <OutDirRoot>$(MSBuildThisFileDirectory)</OutDirRoot>
    <OutDir>$([msbuild]::EnsureTrailingSlash($(OutDirRoot))){project.target_name}\</OutDir>
    <IntDirRoot>$(MSBuildThisFileDirectory)</IntDirRoot>
    <IntDir>$(IntDirRoot)$(RootNamespace)\</IntDir>
    <InstallDirRoot></InstallDirRoot>
    <InstallDir>$([msbuild]::EnsureTrailingSlash($(InstallDirRoot))){project.target_name}\</InstallDir>
  </PropertyGroup>
  <PropertyGroup Label="Internal">
    <_RecordFile>$(IntDir)$(MSBuildThisFileName).RECORD.txt</_RecordFile>
  </PropertyGroup>
