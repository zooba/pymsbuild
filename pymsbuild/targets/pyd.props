<Project>
  <PropertyGroup>
    <TargetExt Condition="$(__TargetExt) != ''">$(__TargetExt)</TargetExt>
    <TargetPath>$([System.IO.Path]::Combine($(ProjectDir),$(OutDir),$(TargetName)$(TargetExt)))</TargetPath>
    <LinkIncremental>false</LinkIncremental>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <AdditionalIncludeDirectories Condition="$(PythonIncludes) != ''">$(PythonIncludes);%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
    </ClCompile>
    <Link>
      <AdditionalLibraryDirectories Condition="$(PythonLibs) != ''">$(PythonLibs);%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
      <GenerateDebugInformation>True</GenerateDebugInformation>
    </Link>
    <Lib>
      <OutputFile>$(TargetPath)</OutputFile>
    </Lib>
  </ItemDefinitionGroup>
</Project>